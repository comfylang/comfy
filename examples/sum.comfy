/// Takes two numbers and returns their sum
/// If no arguments are passed, the default values is 2
fn sum(a = 2, b = 2) -> i8 {
  a + b
}

fn main() -> int {
  let w a = 1; // First argument
  let b = 2; // Second argument
  
  printf("%d", sum(a, b)); // Prints 3
  printf("\n%d\n", sum()); // Prints 4

  0
}